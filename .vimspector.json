{
  "configurations": {
    "Python": {
      "adapter": "debugpy",
      "filetypes": [ "python" ],
      "default": true,
      "configuration": {
        "type": "python",
        "console": "externalTerminal",
        "request": "launch",
        "program": "${workspaceRoot}/debug.py",
        "stopOnEntry": false,
        "cwd": "${workspaceRoot}"
      }
    }
  }
}
